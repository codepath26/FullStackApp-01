<%- include('home.ejs') %>
    <section class="display-section">
      <div class="container">
        <h1>Order Details</h1>
        <ul class="list-group">
          <% details.forEach(detail => { %>
            <li class="list-group-item">
              Name: <%= detail.name %><br>
              Number: <%= detail.number %><br>
              Email: <%= detail.email %>
            </li>
          <% }); %>
        </ul>
      </div>
  </section>
  <%- include('../includes/end.ejs') %>
